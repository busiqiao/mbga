COMPOSE_PROJECT_NAME=mjga-dbfirst
# database
DATABASE_HOST_PORT=host.docker.internal:3306
DATABASE_STORE=~/docker/store/mjga-dbfirst/db/data
DATABASE_DB=mjga
DATABASE_USER=mjga-dbfirst
DATABASE_PASSWORD=mjga-dbfirst
DATABASE_ROOT_PASSWORD=mjga-dbfirst-root
DATABASE_EXPOSE_PORT=3306
DATABASE_INIT=./db.d/init
# web
WEB_EXPOSE_PORT=8080
LOG_PATH=~/docker/store/mjga-dbfirst/log
RESOURCE_LOCATIONS=classpath:public/
RESOURCE_HANDLER=/asset/**

# cors
ALLOWED_ORIGINS=http://localhost,https://localhost
ALLOWED_METHODS=*
ALLOWED_HEADERS=*
# redis
REDIS_HOST=host.docker.internal
REDIS_EXPOSE_PORT=6379
REDIS_PASSWORD=mjga-dbfirst
REDIS_STORE=~/docker/store/mjga-dbfirst/redis/data
# rabbitmq
RABBITMQ_HOST=host.docker.internal
RABBITMQ_DEFAULT_USER=mjga
RABBITMQ_DEFAULT_PASS=mjga
RABBITMQ_PORT=5672
RABBITMQ_UI_PORT=15672
RABBITMQ_DATA_STORE=~/docker/store/mjga-dbfirst/mq/data
